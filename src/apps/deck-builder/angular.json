{
	"$schema": "../../../node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "pnpm"
	},
	"newProjectRoot": "projects",
	"projects": {
		"deck-builder": {
			"projectType": "application",
			"schematics": {},
			"root": "",
			"sourceRoot": "./",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "main.ts",
						"tsConfig": "tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							}
						],
						"styles": ["styles.css"],
						"server": "main.server.ts",
						"outputMode": "static",
						"outputPath": "../../../output/deck-builder"
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all",
							"fileReplacements": [
								{
									"replace": "../../environment/environment.ts",
									"with": "../../environment/environment.production.ts"
								}
							]
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						},
						"lan": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"fileReplacements": [
								{
									"replace": "../../environment/environment.ts",
									"with": "../../environment/environment.lan.ts"
								}
							]
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "deck-builder:build:production"
						},
						"development": {
							"buildTarget": "deck-builder:build:development"
						},
						"lan": {
							"buildTarget": "deck-builder:build:lan"
						}
					},
					"defaultConfiguration": "development"
				}
			}
		}
	}
}
